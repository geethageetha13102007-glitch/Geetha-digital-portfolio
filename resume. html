<!DOCTYPE html>
<html>
<head>
  <title>My Resume</title>
</head>
<body>
  <h1>My Resume</h1>
  <div id="resume"></div>

  <script>
    // Resume stored as a JavaScript object
    const resume = {
      name: "R. Geetha",
      email: "geethageetha13102007@gmail.com",
      phone: "8072505525",
      education: [
        { degree: "B.Sc. Computer Science", school: "XYZ University", year: 2022 }
      ],
      skills: ["JavaScript", "HTML", "CSS"],
      experience: [
        { company: "ABC Corp", role: "Web Developer", years: "2022-2023" }
      ]
    };

    // Display resume on the page
    const resumeDiv = document.getElementById("resume");

    resumeDiv.innerHTML = `
      <h2>${resume.name}</h2>
      <p>Email: ${resume.email}</p>
      <p>Phone: ${resume.phone}</p>
      
      <h3>Education</h3>
      <ul>
        ${resume.education.map(ed => `<li>${ed.degree} - ${ed.school} (${ed.year})</li>`).join('')}
      </ul>

      <h3>Skills</h3>
      <ul>
        ${resume.skills.map(skill => `<li>${skill}</li>`).join('')}
      </ul>

      <h3>Experience</h3>
      <ul>
        ${resume.experience.map(exp => `<li>${exp.role} at ${exp.company} (${exp.years})</li>`).join('')}
      </ul>
    `;
  </script>
</body>
</html>
